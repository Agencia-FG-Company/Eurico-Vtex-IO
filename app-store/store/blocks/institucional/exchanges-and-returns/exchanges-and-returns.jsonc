  "rich-text#institucional__wrapper-exchanges-and-returns--group-answer-third": {
    "title": "Resposta - Dropdown",
    "props": {
      "blockClass":["institucional__wrapper--group-answer","exchanges-and-returns"],
      "text": "A Eurico oferece o frete de devolução na primeira troca (envio econômico).\nEste benefício é válido somente para a primeira troca"
    }
  },





