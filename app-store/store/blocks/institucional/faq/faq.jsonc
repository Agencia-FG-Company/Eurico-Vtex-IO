{
  "store.custom#faq": {
    "children": ["flex-layout.row#institucional__wrapper-faq"]
  },

  "flex-layout.row#institucional__wrapper-faq": {
    "props": {
      "blockClass": "institucional__wrapper"
    },
    "children": [
      "flex-layout.col#institucional__wrapper-left-faq",
      "flex-layout.col#institucional__wrapper-right-faq"
    ]
  },

  "flex-layout.col#institucional__wrapper-right-faq": {
    "props": {
      "blockClass": "institucional__wrapper-right"
    },
    "children": [
      "rich-text#faq__title",
      "flex-layout.row#institucional__wrapper-faq--content"
    ]
  },
  "flex-layout.col#institucional__wrapper-left-faq": {
    "props": {
      "blockClass": "institucional__wrapper-left"
    },
    "children": [
     "responsive-layout.mobile#menu-mobile--faq","responsive-layout.desktop#menu-desktop--faq"
    ]
  },


  "rich-text#faq__title": {
    "props": {
      "blockClass": "institucional__wrapper-right--title",
      "text": "# PERGUNTAS FREQUENTES"
    }
  },

  "flex-layout.row#institucional__wrapper-faq--content": {
    "title": "Blocos",
    "props": {
      "blockClass": "institucional__wrapper-faq--content"
    },
    "children": [
      "link#institucional__wrapper-faq--col",
      "link#institucional__wrapper-faq--col-2",
      "link#institucional__wrapper-faq--col-3",
      "link#institucional__wrapper-faq--col-4",
      "link#institucional__wrapper-faq--col-5",
      "link#institucional__wrapper-faq--col-6"
    ]
  },

  "link#institucional__wrapper-faq--col": {
    "props": {
      "href": "/institucional/como-comprar"
    },
    "children": ["flex-layout.col#institucional__wrapper-faq--col"]
  },

  "flex-layout.col#institucional__wrapper-faq--col": {
    "props": {
      "blockClass": "institucional__wrapper-faq--col"
    },
    "children": [
      "image#institucional__wrapper-faq--col-img",
      "rich-text#institucional__wrapper-faq--col-text"
    ]
  },
  "rich-text#institucional__wrapper-faq--col-text": {
    "title": "Texto do bloco",
    "props": {
      "blockClass": "institucional__wrapper-faq--col-text",
      "text": "COMO COMPRAR"
    }
  },
  "image#institucional__wrapper-faq--col-img": {
    "title": "Imagem do bloco",
    "props": {
      "blockClass": "institucional__wrapper-faq-img",
      "src": "assets/images/institucional/faq/comprar.png"
    }
  },

