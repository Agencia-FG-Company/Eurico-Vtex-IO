{
  "vtex.store-components:product-name#product__info--name": {
    "props": {
      "blockClass": "product__info--name"
    }
  },
  "flex-layout.row#product-name": {
    "props": {
      "marginBottom": 3,
      "blockClass": "product__name-pdp"
    },
    "children": [
      "vtex.store-components:product-name#product__info--name"
    ]
  },
  "flex-layout.row#buy-button": {
    "props": {
      "marginTop": 4,
      "marginBottom": 7
    },
    "children": [
      "add-to-cart-button"
    ]
  },
  "flex-layout.row#product-availability": {
    "props": {
      "colGap": 7,
      "marginTop": 4,
      "marginBottom": 7,
      "paddingTop": 7,
      "blockClass": "product-main"
    },
    "children": [
      "flex-layout.col#stack",
      "flex-layout.col#right-col-availability"
    ]
  },
  "flex-layout.col#right-col-availability": {
    "props": {
      "preventVerticalStretch": true,
      "rowGap": 0,
      "blockClass": "info-availability"
    },
    "children": [
      "flex-layout.row#product-name",
      "flex-layout.row#product__ratingAndref",
      "product-list-price#pdp",
      "flex-layout.row#product__price-pdp",
      "modal-trigger#product-payment-options",
      "product-separator",
      "fg-product-variants",
      "sku-selector#pdp",
      "product-assembly-options",
      "product-gifts",
      "modal-trigger#product__guide-size",
      "flex-layout.row#availability",
      "fg-product-description",
      "product-separator",
      "share#default",
      "flex-layout.row#product-advantages",
      "fg-custom-buy-too"
      // "product-identifier.product",
    ]
  },
  "flex-layout.row#availability": {
    "props": {
      "blockClass": "message-availability"
    },
    "children": [
      "availability-notify"
    ]
  },
  "flex-layout.row#product__ratingAndref": {
    "props": {
      "blockClass": "product__ratingAndref"
    },
    "children": [
      "fg-CustomProductRef",
      "product-rating-summary"
    ]
  },
  "share#default": {
    "props": {
      "social": {
        "Facebook": false,
        "WhatsApp": true,
        "Twitter": false,
        "Pinterest": false
      },
      "blockClass": "share__pdp"
    }
  }
}
